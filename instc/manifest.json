{
   "background": {
      "persistent": true,
      "scripts": [ "/js/background.js" ]
   },
   "browser_action": {
      "default_title": "Open a instagram post page, then click the button"
   },
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "description": "Export all Comments from a Instagram Post to CSV Excel & Giveaways Picker",
   "icons": {
      "128": "InsC-128.png",
      "16": "InsC-16.png",
      "48": "InsC-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "InsC - Instagram Comments Exporter & Picker",
   "permissions": [ "*://*.instagram.com/*", "*://*.cdninstagram.com/*", "*://*.fbcdn.net/*", "storage", "downloads", "webRequest", "webRequestBlocking" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.7"
}
